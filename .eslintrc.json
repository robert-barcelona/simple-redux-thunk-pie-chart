{
  "extends": ["eslint:recommended","plugin:react/recommended", "prettier", "prettier/react"],
  "plugins": ["jest"],

  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType":"module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "jest/globals": true,
    "es6": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react" : {
      "version": "detect"
    }
  }
}
